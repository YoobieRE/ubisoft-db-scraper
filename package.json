{
  "name": "ubisoft-db-scraper",
  "version": "0.1.0",
  "description": "A bot that pulls data from the Ubisoft Connect Demux API",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start:ts": "ts-node src/index.ts",
    "start:db": "docker run --name mongodb -d -p 27017:27017 -v $(pwd)/db:/data/db mongo",
    "docker:build": "docker build . -t ubisoft-db-scraper",
    "docker:run": "docker run --rm -v $(pwd)/config:/config ubisoft-db-scraper",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Charlie Laabs",
    "url": "https://github.com/claabs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YoobieRE/ubisoft-db-scraper.git"
  },
  "keywords": [
    "ubisoft",
    "ubi",
    "ubisoft connect",
    "uplay",
    "protobuf",
    "protocol buffer",
    "dmx",
    "demux",
    "database",
    "scraper"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist/src"
  ],
  "dependencies": {
    "bottleneck": "^2.19.5",
    "dotenv": "^16.0.1",
    "fast-deep-equal": "^3.1.3",
    "fs-extra": "^10.1.0",
    "mongoose": "^6.5.1",
    "node-schedule": "^2.1.0",
    "otpauth": "^8.0.1",
    "pino": "^8.4.0",
    "pino-pretty": "^8.1.0",
    "simple-git": "^3.11.0",
    "source-map-support": "^0.5.21",
    "ubisoft-demux": "^1.10690.5",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.47",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
